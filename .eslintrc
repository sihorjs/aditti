{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "plugins": ["react"],
    "parserOptions": {
        "ecmaVersion": 7,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "camelcase": "error",
        "indent": "error",
        "quotes": "error",
        "object-curly-spacing": ["error", "always"]
    },
    "env": {
        "es6": true,
        "node": true
    }
}
